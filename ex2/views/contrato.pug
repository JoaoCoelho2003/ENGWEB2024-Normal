extends layout

block content
  .w3-container.w3-padding-32
    .w3-card-4.w3-round-large
      header.w3-container.w3-teal.w3-round-large
        h1(class='w3-center w3-margin-0') Detalhes do Contrato
      .w3-container.w3-padding-16
        table.w3-table.w3-striped.w3-border
          each value, key in contrato
            tr
              td.w3-text-grey-800 #{key}
              td.w3-text-black #{value}
      footer.w3-container.w3-teal.w3-padding.w3-round-large
        address.w3 Desenvolvido por Jo√£o Coelho em #{date} - 
          a(href='/') Voltar
